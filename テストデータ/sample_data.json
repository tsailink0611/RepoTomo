  "api_endpoints": {
    "base_url": "https://script.google.com/macros/s/{SCRIPT_ID}/exec",
    "endpoints": [
      {
        "path": "/",
        "method": "GET",
        "description": "スタッフ用PWAアプリの表示",
        "parameters": {
          "page": "staff"
        }
      },
      {
        "path": "/",
        "method": "GET",
        "description": "管理者用ダッシュボードの表示",
        "parameters": {
          "page": "admin"
        }
      },
      {
        "path": "/",
        "method": "POST",
        "description": "出勤記録",
        "parameters": {
          "action": "checkin",
          "userId": "string"
        }
      },
      {
        "path": "/",
        "method": "POST",
        "description": "退勤記録",
        "parameters": {
          "action": "checkout",
          "userId": "string"
        }
      },
      {
        "path": "/",
        "method": "POST",
        "description": "勤怠データ取得",
        "parameters": {
          "action": "getAttendance",
          "userId": "string",
          "startDate": "YYYY-MM-DD",
          "endDate": "YYYY-MM-DD"
        }
      }
    ]
  },
  "test_scenarios": [
    {
      "name": "正常な出勤・退勤フロー",
      "steps": [
        "スタッフがLINEボットに「出勤」とメッセージを送信",
        "システムが出勤時刻を記録",
        "スタッフに確認メッセージを返信",
        "8時間後にスタッフが「退勤」とメッセージを送信",
        "システムが退勤時刻を記録し、勤務時間を計算",
        "スタッフに勤務時間を含む確認メッセージを返信"
      ]
    },
    {
      "name": "重複出勤の防止",
      "steps": [
        "スタッフが既に出勤済みの状態で再度「出勤」とメッセージを送信",
        "システムが既に出勤済みであることを検知",
        "スタッフに既に出勤済みである旨のメッセージを返信"
      ]
    },
    {
      "name": "管理者ダッシュボードでの確認",
      "steps": [
        "管理者がWebブラウザで管理画面にアクセス",
        "リアルタイムでスタッフの出勤状況を確認",
        "勤怠データをCSVエクスポート",
        "月次レポートを生成"
      ]
    }
  ]
